{
  "name": "server",
  "version": "1.0.0",
  "description": "Server",
  "main": "src/main.ts",
  "private": true,
  "license": "Private",
  "scripts": {
    "build": "yarn clean && yarn webpack:build",
    "serve": "node build/index.js --max_old_space_size=4096",
    "serve:babel": "babel-node build/index.js --max_old_space_size=4096",
    "serve:stage": "NODE_ENV=staging node build/index.js",
    "analyze": "webpack --progress --config webpack.prod.js --json | webpack-bundle-size-analyzer",
    "type-check": "tsc -b",
    "clean": "rimraf build",
    "webpack:build": "node ../../node_modules/webpack/bin/webpack.js --config webpack.prod.js",
    "dev:build-server-once": "webpack --config webpack.dev.js",
    "dev:run-server": "node-dev ./build",
    "dev:watch-server": "webpack --config webpack.dev.js --watch",
    "start": "yarn clean && yarn dev:build-server-once && npm-run-all --parallel dev:run-server dev:watch-server",
    "dev:start": "yarn clean && yarn dev:build-server-once && yarn dev:run-server",
    "format": "prettier --write .",
    "prestart": "prisma generate --schema ../../../../libs/clients_db/schema.prisma",
    "create:module": "ts-node ./scripts/module/cli.ts create"
  },
  "dependencies": {
    "@prisma/client": "5.16.1",
    "axios": "1.6.0",
    "bcrypt": "5.1.1",
    "chalk": "4.1.2",
    "commander": "12.1.0",
    "compression": "1.7.4",
    "dd-trace": "5.17.0",
    "dotenv": "16.4.5",
    "express": "4.18.1",
    "express-validator": "7.1.0",
    "helmet": "7.1.0",
    "http-status-codes": "2.3.0",
    "husky": "9.0.11",
    "inquirer": "8.2.6",
    "inversify": "6.0.2",
    "inversify-express-utils": "6.4.6",
    "jsonwebtoken": "9.0.2",
    "mongo-migrate-ts": "1.6.0",
    "oauth2-server": "3.1.1",
    "prisma": "5.16.1",
    "reflect-metadata": "0.2.2",
    "tslib": "2.3.0",
    "underscore": "1.13.6",
    "winston": "3.13.0"
  },
  "devDependencies": {
    "@types/bcrypt": "5.0.2",
    "@types/compression": "1.7.5",
    "@types/dd-trace": "0.9.0",
    "@types/express": "4.17.21",
    "@types/inquirer": "9.0.7",
    "@types/jsonwebtoken": "9.0.6",
    "@types/oauth2-server": "3.0.17",
    "@types/underscore": "1.11.15",
    "ts-node": "10.9.1"
  }
}
